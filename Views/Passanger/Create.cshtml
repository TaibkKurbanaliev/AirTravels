@model AirTravels.ViewModels.CreatePassangerViewModel

<form method="post" asp-action="Create">
    <div class="form-group">
        <label for="exampleInputEmail1">Фамилия</label>
        <input asp-for="SecondName" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Фамилия">
    </div>
    <div class="form-group">
        <label for="exampleInputEmail1">Имя</label>
        <input asp-for="FirstName" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Имя">
    </div>
    <div class="form-group">
        <label for="exampleInputEmail1">Отчество</label>
        <input asp-for="ThirdName" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Отчество">
    </div>
    <div class="form-group">
        <label for="exampleInputEmail1">тип документа</label>
        <select asp-for="Document.Type">
            @foreach (var document in Model.DocumentTypes)
            {
                <option value="@document.Id">@document.Type</option>
            }
        </select>
    </div>
    <div class="form-group">
        <label for="exampleInputEmail1">номер документа</label>
        <input asp-for="Document.Number" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Enter email">
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>